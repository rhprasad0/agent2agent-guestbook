# AWS Configuration
aws_region = "us-east-1"

# Resource Names
dynamodb_table_name = "a2a-guestbook-messages"
secret_name         = "a2a-guestbook/api-keys"

# Initial API Keys (generate secure keys!)
# Use: openssl rand -hex 32
initial_api_keys = [
  "agent-1-replace-with-secure-key",
  "agent-2-replace-with-secure-key",
  "agent-3-replace-with-secure-key"
]

# Tags
tags = {
  Project     = "A2A-Guestbook"
  ManagedBy   = "Terraform"
  Environment = "dev"
}
